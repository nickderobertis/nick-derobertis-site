{
    "family": "nick-derobertis-site-task-description",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "nick-derobertis-site-main-app",
            "image": "nick-derobertis-site:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [
                "sh",
		"-c"
            ],
            "command": [
                "/bin/sh -c pipenv run python nick_derobertis_site/home.py"
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}